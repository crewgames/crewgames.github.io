# EditorConfig file

root = false

[*]
indent_style = space
indent_size = 4
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[node_modules/**]
indent_style = ignore
indent_size = ignore
end_of_line = ignore
trim_trailing_whitespace = ignore
insert_final_newline = ignore

[assets/ie/**.{htc,js,md}]
indent_style = ignore
indent_size = ignore
end_of_line = ignore
trim_trailing_whitespace = ignore
insert_final_newline = ignore

[{.eslintignore,.eslintrc,.gitattributes,.gitignore,CNAME,LICENSE}]
indent_style = ignore
indent_size = ignore
end_of_line = ignore
trim_trailing_whitespace = ignore
insert_final_newline = ignore

[**.{txt,md,svg}]
indent_style = ignore
indent_size = ignore
end_of_line = ignore
trim_trailing_whitespace = ignore
insert_final_newline = ignore
